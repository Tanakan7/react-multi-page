{
  "name": "react-multi-page",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "",
  "author": "kanji",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --config config/conf.dev.js --mode development",
    "build": "rimraf dist && webpack --config config/conf.prod.js --mode production",
    "post-build": "rimraf dist/js/*.js.LICENSE.txt && find ./dist -name \".DS_Store\" | xargs rm && zip -r dist/dist ./dist/*",
    "build:stage": "npm run build -- --deploy stage && npm run post-build",
    "build:prod": "npm run build -- --deploy prod && npm run post-build",
    "build:release": "npm run build -- --deploy release && npm run post-build",
    "format:js": "prettier-eslint --write $PWD/'src/**/*.js' --parser babel",
    "format:jsx": "prettier-eslint --write $PWD/'src/**/*.jsx' --parser babel",
    "format:scss": "stylelint --fix './src/**/*.scss'",
    "lint": "npm run format:jsx & npm run format:js && eslint 'src/**/*.{js,jsx}'",
    "pre-commit": "npm run format:scss & npm run lint",
    "view": "http-server ./dist -p 3100 -o",
    "analyze": "npm run build -- --doAnalyze true",
    "mock": "json-server --watch config/mock/searchResult.json --port 5000"
  },
  "browserslist": [
    "iOS >= 11",
    "Android >= 5.0",
    "last 2 ChromeAndroid versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "ejs": "^3.1.5",
    "ejs-easy-loader": "^0.1.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-unsafe-regex": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^5.1.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "husky": "^4.2.5",
    "json-server": "^0.16.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-watch": "^0.6.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.3.2",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.16.1",
    "stylelint-webpack-plugin": "^1.2.3",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^5.1.4"
  },
  "dependencies": {
    "core-js": "3",
    "emotion": "^10.0.27",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "stickybits": "^3.7.7"
  }
}
